{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Posts",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "versions": {
      "versioned": true
    },
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "slug": {
      "pluginOptions": {
        "versions": {
          "versioned": true
        },
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "targetField": "postTitle",
      "required": false
    },
    "postTitle": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "subHeading": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "featuredImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "postCategory": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::post-category.post-category"
    },
    "listingPageDescription": {
      "pluginOptions": {
        "versions": {
          "versioned": true
        },
        "i18n": {
          "localized": true
        }
      },
      "type": "text",
      "required": true
    },
    "postAuthors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::post-author.post-author"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "versions": {
          "versioned": true
        },
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.seo"
    },
    "block": {
      "pluginOptions": {
        "versions": {
          "versioned": true
        },
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": [
        "blocks.rich-text",
        "blocks.call-to-action",
        "blocks.shareable-takeaway",
        "blocks.subscribe-to-blog",
        "blocks.code-snippet",
        "blocks.video",
        "blocks.image"
      ],
      "required": true
    }
  }
}
